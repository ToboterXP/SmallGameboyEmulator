#include "MemorySection.h"

namespace memory {

MemorySection::MemorySection(uint16_t start, uint16_t length) {
	this->memoryLength = length;
	this->memoryStart = start;
	this->memory = new uint8_t[length];
}

MemorySection::~MemorySection() {
	delete this->memory;
	this->isValid = false;
}

} /* namespace memory */
